{
    "name": "svelte-app",
    "version": "1.0.0",
    "scripts": {
        "build": "rollup -c",
        "watch": "rollup -c -w",
        "dev": "run-p start:dev watch",
        "start:dev": "nodemon --watch backend -r esm backend/server.js",
        "start": "node -r esm backend/server.js"
    },
    "devDependencies": {
        "nodemon": "^1.19.3",
        "npm-run-all": "^4.1.5",
        "rollup": "^1.12.0",
        "rollup-plugin-commonjs": "^10.0.0",
        "rollup-plugin-livereload": "^1.0.0",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-svelte": "^5.0.3",
        "rollup-plugin-terser": "^4.0.4",
        "svelte": "^3.0.0"
    },
    "dependencies": {
        "compression": "^1.7.4",
        "esm": "^3.2.25",
        "polka": "^0.5.2",
        "sirv": "^0.4.2",
        "svelte-routing": "^1.4.0"
    }
}
